# =====================================================
# CLASSES
# =====================================================

tto:User
	rdf:type rdfs:Class;
	rdfs:label "User"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Product
	rdf:type rdfs:Class;
	rdfs:label "Product"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Order
	rdf:type rdfs:Class;
	rdfs:label "Order"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Payment
	rdf:type rdfs:Class;
	rdfs:label "Payment"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:ShoppingCart
	rdf:type rdfs:Class;
	rdfs:label "Shopping Cart"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Inventory
	rdf:type rdfs:Class;
	rdfs:label "Inventory"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Shipping
	rdf:type rdfs:Class;
	rdfs:label "Shipping"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Review
	rdf:type rdfs:Class;
	rdfs:label "Review"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Notification
	rdf:type rdfs:Class;
	rdfs:label "Notification"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:Discount
	rdf:type rdfs:Class;
	rdfs:label "Discount"^^xsd:string;
	rdfs:isDefinedBy tto: .

# Enumerations
tto:OrderStatus
	rdf:type rdfs:Class;
	rdfs:label "Order Status"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:PaymentMethod
	rdf:type rdfs:Class;
	rdfs:label "Payment Method"^^xsd:string;
	rdfs:isDefinedBy tto: .

tto:UserRole
	rdf:type rdfs:Class;
	rdfs:label "User Role"^^xsd:string;
	rdfs:isDefinedBy tto: .

# =====================================================
# ENUM VALUES
# =====================================================

ttr:PENDING rdf:type tto:OrderStatus .
ttr:COMPLETED rdf:type tto:OrderStatus .
ttr:CANCELLED rdf:type tto:OrderStatus .

ttr:CREDIT_CARD rdf:type tto:PaymentMethod .
ttr:PAYPAL rdf:type tto:PaymentMethod .
ttr:BANK_TRANSFER rdf:type tto:PaymentMethod .

ttr:ADMIN rdf:type tto:UserRole .
ttr:CUSTOMER rdf:type tto:UserRole .
ttr:GUEST rdf:type tto:UserRole .

